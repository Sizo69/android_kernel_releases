[
  {
    "kernelSource": {
      "name": "lLVM KING",
      "repo": "https://github.com/llvm/llvm-project/releases/download/llvmorg-19.1.5/clang+llvm-19.1.5-aarch64-linux-gnu.tar.xz",
      "branch": "beta",
      "device": "alioth",
      "defconfig": "alioth_defconfig"
    },
    "withKernelSU": false,
    "toolchains": [
      {
        "repo": "https://gitlab.com/kuroringo901/clang-mandisa.git",
        "branch": "uvite",
        "name": "clang",
        "binPath": ["bin"]
      },
      {
        "repo": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9",
        "branch": "android10-release",
        "name": "gcc",
        "binPath": ["bin"]
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang/bin/clang",
      "externalCommand": {
        "LLVM": "1",
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
        "CROSS_COMPILE_COMPAT": "arm-linux-androideabi-"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/Sizo69/AnyKernel3_alioth.git",
      "branch": "master"
    }
  }
]
