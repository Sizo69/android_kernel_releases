[
  {
    "kernelSource": {
      "name": "Boot",
      "repo": "https://github.com/Shirayuki39/kernel_new.git",
      "branch": "mahiru-rebase",
      "device": "mondrian",
      "defconfig": "gki_defconfig"
    },
    "withKernelSU": false,
    "toolchains": [
      {
        "repo": "https://gitlab.com/kuroringo901/clang-mandisa.git",
        "branch": "uvite",
        "name": "clang",
        "binPath": ["bin"]
      },
      {
        "repo": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9",
        "branch": "android10-release",
        "name": "gcc",
        "binPath": ["bin"]
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang/bin/clang",
      "externalCommand": {
        "LLVM": "1",
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
        "CROSS_COMPILE_COMPAT": "arm-linux-androideabi-"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/Sizo69/AnyKernel3_alioth.git",
      "branch": "mondrian"
    }
  }
]
